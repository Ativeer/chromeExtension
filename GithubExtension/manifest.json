{
    "manifest_version": 3,
    "name": "GitHub Commit Tracker",
    "version": "1.0",
    "description": "Track your GitHub commits daily/weekly.",
    "permissions": ["alarms", "notifications", "storage"],
    "host_permissions": ["https://script.google.com/macros/s/*", "https://script.googleusercontent.com/macros/*"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "github-mark.png"
    }
  }